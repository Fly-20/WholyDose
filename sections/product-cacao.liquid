<div class="science-area">

    {% for block in section.blocks %}
    {% if product.handle == block.settings.theproduct %}

      
      <div class="science-info">
        <h1>THE SCIENCE</h1>

        {% if block.settings.sciencehtml  != blank %}
          <div style="padding: 15px 0;">{{ block.settings.sciencehtml }}</div>
        {% endif %}


        {% if block.settings.icon1  != blank %}
          <div class="icon">
            <img src="{{ block.settings.icon1 | img_url: '300x' }}">
            <h1>{{ block.settings.icon1title }}</h1>
          </div>
          {% endif %}

          {% if block.settings.icon2  != blank %}
          <div class="icon" style="padding-left: 3px;">
            <img src="{{ block.settings.icon2 | img_url: '300x' }}">
            <h1>{{ block.settings.icon2title }}</h1>
          </div>
          {% endif %}

          {% if block.settings.icon3  != blank %}
          <div class="icon" style="padding-left: 3px;">
            <img src="{{ block.settings.icon3 | img_url: '300x' }}">
            <h1>{{ block.settings.icon3title }}</h1>
          </div>
          {% endif %}

          {% if block.settings.icon4  != blank %}
          <div class="icon" style="padding-left: 3px;">
            <img src="{{ block.settings.icon4 | img_url: '300x' }}">
            <h1>{{ block.settings.icon4title }}</h1>
          </div>
          {% endif %}

          {% if block.settings.icon5  != blank %}
          <div class="icon" style="padding-left: 5px;">
            <img src="{{ block.settings.icon5 | img_url: '300x' }}">
            <h1>{{ block.settings.icon5title }}</h1>
          </div>
          {% endif %}

          {% if block.settings.icon6  != blank %}
          <div class="icon">
			<img style="margin-top: -8px;" src="{{ block.settings.icon6 | img_url: '300x' }}" >
            <h1>{{ block.settings.icon6title }}</h1>
          </div>
          {% endif %}

      </div>

      <div class="science-img">
        <img  src="{{ block.settings.scienceimg | img_url: '1200x' }}">
      </div>


    {% endif %}
  {% endfor %}


</div>


