<div class="faq-area">
    {% for block in section.blocks %}
    {% if product.handle == block.settings.theproduct %}
  <h1>FREQUENTLY ASKED QUESTIONS:</h1>

      {% if block.settings.q1  != blank %}
      <div class="faq-box prod-faq">
        <h2 class="title waypoint">{{ block.settings.q1 }}</h2>
        <div class="content">
          {{ block.settings.a1 }}
        </div>
      </div>
      {% endif %}
      {% if block.settings.q2  != blank %}
