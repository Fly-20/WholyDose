{% for block in section.blocks %}
{% if product.handle == block.settings.theproduct %}

<div class="ziz-related cf">
  <h2>You may also like...</h2>
  {% assign myTitle = product.title %}
  {% for product in collections[block.settings.upsell].products limit: 4 %}
    
    {% unless  product.title == myTitle %}
    <a href="{{ product.url }}" class="prod-box">
      <img src="{{ product.featured_image.src | img_url: '600x' }}" alt="{{ product.featured_image.alt | escape }}">
      <h1>{{ product.title }}</h1>
    </a>
    {% endunless %}

  {% endfor %}
</div>

{% endif %}
{% endfor %}


