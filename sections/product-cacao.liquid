<div class="feat-blog">
  
  {% for block in section.blocks %}
    {% if product.handle == block.settings.theproduct %}

      <!-- Flickity HTML init -->
      <div class="carousel blog-slider">

      <div class="carousel-cell recipe-block">
      
        {% assign thearticle = articles[block.settings.thearticle] %}
        <div class="meta">
          <h1>RECIPE IDEAS</h1>
          <h2>{{ thearticle.title }}</h2>
          <div class="rte">
            {% if article.excerpt == blank %}
            {{ article.content | strip_html | truncatewords: 30 }}
            {% else %}
            {{ article.excerpt }}
            {% endif %}
          </div>
          <a  href="{{ thearticle.url }}" class="btn readmore">GET RECIPE</a>
        </div>
        <div class="the-img">
